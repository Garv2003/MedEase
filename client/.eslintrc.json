{
  "plugins": ["import"],
  "extends": [
    "next/core-web-vitals",
    "standard",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "rules": {
    "no-undef": "off",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ]
        // "alphabetize": {
        // "order": "asc",
        // "caseInsensitive": true
        // }
      }
    ]
  }
}
